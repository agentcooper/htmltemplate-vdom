{
  "name": "htmltemplate-vdom",
  "version": "0.5.2",
  "description": "HTML::Template engine on top of virtual-dom",
  "keywords": [
    "htmltemplate",
    "HTML::Template",
    "template",
    "virtual",
    "dom"
  ],
  "author": "Artem Tyurin <artem.tyurin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentcooper/htmltemplate-vdom"
  },
  "bin": "bin/compile",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "precommit": "npm test",
    "prepush": "npm test",
    "build-production": "NODE_ENV=production webpack -p",
    "precompile": "./bin/compile --path example/precompiled/tmpl.inc --hooks=1 > example/precompiled/template.js; cat example/precompiled/template.js | uglifyjs -c -m > example/precompiled/template.min.js"
  },
  "license": "MIT",
  "dependencies": {
    "colors": "^1.1.2",
    "escodegen": "^1.6.1",
    "htmltemplate-parser": "^0.3.0",
    "hyperscript": "^1.4.6",
    "lodash.flatten": "^3.0.2",
    "main-loop": "^3.1.0",
    "traverse": "^0.6.6",
    "virtual-dom": "^2.1.1",
    "yargs": "^3.23.0"
  },
  "devDependencies": {
    "brfs": "^1.4.1",
    "transform-loader": "^0.2.2",
    "husky": "^0.9.3",
    "json-loader": "^0.5.2",
    "mocha": "2.3.2",
    "raw-loader": "^0.5.1"
  }
}
